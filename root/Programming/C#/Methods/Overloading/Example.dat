class Program
{
    private static int Join(int a, int b)
    {
        return a + b;
    }
    
    private static string Join(string a, string b)
    {
        return a + b;
    }
    
    public static void Main()
    {
        int sum23 = Join(2, 3); // #=> 5
        string concat23 = Join("2", "3"); // #=> 23
    }
}